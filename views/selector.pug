extends layout

block content
  h1= title
  - let image_names = ['RR1.jpg','RR2.jpg','RR3.jpg','RR4.jpg','RR5.jpg']
  - for(let i=0;i<3;i++){
      - let randomNumber = Math.floor(Math.random()*5)
      - let imageNameVariable = image_names[randomNumber]

      img(src=`images/${imageNameVariable}` alt='RollsRoyce' style='width:300px; height:300px;')
      |    
  - }